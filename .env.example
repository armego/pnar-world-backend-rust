# Local development environment variables
# Copy this to .env and fill in your local credentials.
# This file should be in your .gitignore and NOT committed to version control.

# PostgreSQL Database
DATABASE_USER="postgres"
DATABASE_PASSWORD="your_local_db_password"
DATABASE_URL="postgres://postgres:your_local_db_password@127.0.0.1:5432/pnar_world"

# JWT Secret - Use a long, random string
JWT_SECRET="a_very_long_and_random_secret_string_for_local_dev"

# Application Environment
APP_ENVIRONMENT="development"
